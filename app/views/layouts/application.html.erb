<!DOCTYPE html>
<html>
  <head>
    <title>LittleFacebook</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
     <% if current_user %>
    <%= link_to 'Profile page', user_path(current_user) %> | <%= link_to "Log out", sessions_logout_path %>
    <%else%>
    <%= link_to 'Register', new_user_path %> | <%= "Log In", root_path %>
    <%end%>
    <%= yield %>
  </body>
</html>
